@model List<hypster_tv_DAL.Chart>
@{
    Layout = null;
}<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://hypster.com/main-sitemap.xsl"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml">
@foreach (var item in Model)
{
    <url>
        <loc>http://hypster.com/charts/details/@item.Chart_GUID</loc>
        <lastmod>@ViewBag.ModDate</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.60</priority>
    </url>
    
    <url>
        <loc>http://hypster.com/playlists/chart/@item.Chart_GUID</loc>
        <xhtml:link rel="alternate" media="only screen and (max-width: 640px)" href="http://hypster.com/apps/appPlaylists/chart/@item.Chart_GUID" />
        <lastmod>@ViewBag.ModDate</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.60</priority>
    </url>
}
</urlset>