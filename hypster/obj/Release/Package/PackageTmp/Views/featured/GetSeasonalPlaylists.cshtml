@model List<hypster_tv_DAL.FeaturedContent>
@{
    Layout = null;
}



<div class="ftrdArtistsCH1">

    @{ int counter = 0; }
    @foreach(var item in Model)
    {
        if (counter == 12)
        {
            <div class="dvdr1"></div>
            counter = 0;
        }
        
        <div class="fitmH1">
            <img alt="" src="/imgs/home/home_slideshow/@item.fc_image" onclick="@item.fc_href" />
            <div>@item.fc_name</div>
        </div>
        
        
        counter = counter + 1;
    }

</div>
