@model List<hypster_tv_DAL.DynamicContent>
@{
    Layout = null;
}




@{ int i_counter = 0; }

@foreach (var item in Model)
{   
    @Html.Raw(item.DynCont_BoxCont)
    
    
    <div class="boxContLeft">
        <div class="slideLeft" style=" min-height:35px;">
               <div class="NPFT_GoOnTop" onclick="$(document).scrollTop($(document).height() - $(window).height() - 20);" style=" float:right; width:274px; height:30px; line-height:30px;"> Go To Bottom </div>
               <div class="NPFT_GoOnTop" onclick="$(document).scrollTop(0);" style=" float:right; width:274px; height:30px; line-height:30px;"> Go To Top </div>
        </div>
    </div>
    
    i_counter += 1;
}




@if (Model.Count > 0)
{
    <script type="text/javascript">
        RenderPage();
    </script>
}