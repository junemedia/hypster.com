@model List<hypster_tv_DAL.PlaylistData_Song>
@{
    Layout = null;
}






@if (ViewBag.LAYOUT == 2)
{
    <div class="skn1VideoBack">
        <div id="sSongPlayer" style=" float:left; width:1010px; height:480px; margin:10px; background-color:#111111;"></div>
    </div>

    for (int i = 0; i < Model.Count; i++)
    {
        <div class="skn1songBack">
            <img alt="" src="http://i.ytimg.com/vi/@Model[i].YoutubeId/0.jpg" />
            <div>@Model[i].Title</div>
        </div>
    }
 
}
else
{
    
    for (int i = 0; i < Model.Count; i++)
    {
        <img class="sknRimg" alt="" src="http://i.ytimg.com/vi/@Model[i].YoutubeId/0.jpg" />
    }
    
}