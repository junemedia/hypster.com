<!DOCTYPE html>
<html>
<head>
    @if (Request.Browser.IsMobileDevice)
    {
        if (Request.Path.Contains('/') && Request.Path != "/")
        {
            string sq_str = "";
            if (Request.QueryString != null)
            {
                sq_str = "?" + Request.QueryString.ToString();
            }
            Response.Redirect("/apps/app" + Request.Path.Remove(0, 1) + sq_str);
        }
        else
        {
            Response.Redirect("/apps/appHome");
        }
    }
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="alexaVerifyID" content="D1zuW6QmyfkVJqITiYkg7ecUz5k" />
    <meta name="description" content="@ViewBag.Desc" />    
    
    @if (ViewBag.LinkTag != null)
    {
        <text>@Html.Raw(ViewBag.LinkTag)</text>
    }
    
    <link src="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css"  rel="stylesheet" type="text/css" />
    
    @Styles.Render("~/bundles/css")

    <script src="http://code.jquery.com/jquery-1.8.3.min.js"  type="text/javascript"></script>
