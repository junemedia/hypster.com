    @if (Request.Browser.IsMobileDevice)
    {
        if (Request.Path.Contains('/') && Request.Path != "/")
        {
            string sq_str = "";
            if (Request.QueryString != null)
            {
                sq_str = "?" + Request.QueryString.ToString();
            }
            Response.Redirect("/apps/app" + Request.Path.Remove(0, 1) + sq_str);
        }
    }
