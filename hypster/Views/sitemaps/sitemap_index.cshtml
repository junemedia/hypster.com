@model List<hypster_tv_DAL.Sitemap_Content>
@{
    Layout = null;
}<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://hypster.com/main-sitemap.xsl"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    @foreach (var item in Model)
    {
        DateTime dt = (DateTime)item.Sitemap_Updated;
        <sitemap>
            <loc>@item.Sitemap_URL</loc>
            <lastmod>@dt.ToString("s")+00:00</lastmod>
        </sitemap>
    }
</sitemapindex>
<!-- XML Sitemap generated by Hypster -->
