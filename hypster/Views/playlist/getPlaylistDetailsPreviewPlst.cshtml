@model List<hypster_tv_DAL.PlaylistData_Song>
@{
    Layout = null;
}



@{ int counter = 0; }
@for (int i = 0; i < Model.Count; i++)
{
    <img title="@Model[i].Title" src="http://i.ytimg.com/vi/@Model[i].YoutubeId/0.jpg" class="prPlstP" />
    
    if (counter > 5)
    {
        break;
    }
    counter++;
}
