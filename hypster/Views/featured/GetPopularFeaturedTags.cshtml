@model List<hypster_tv_DAL.Tag>
@{
    Layout = null;
}

         
<div class="ftrdTagsCH1">
                
    @for(var i=0; i<Model.Count; i++)
    {   
        <a href="/tags/@HttpUtility.UrlEncode(Model[i].Tag_Name)">@Model[i].Tag_Name</a>
            
        if (i == 20)
        {
            break;
        }
    }

</div>






