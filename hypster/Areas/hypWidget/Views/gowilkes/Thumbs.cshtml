@model List<hypster_tv_DAL.AolSeedVideo>
@{  
    Layout = null;
}

<!DOCTYPE html>
<html>



<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>www.gowilkes.com</title>
    <meta name="description" content="" />


    <script src="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css"  type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-1.8.3.min.js"  type="text/javascript"></script>
    <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"  type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>


    <link href="/Areas/hypWidget/css/goWilkesSPL.css?id=13287293209" rel="stylesheet" type="text/css" />

</head>





<body margintop="0" style=" margin:0; padding:0px; overflow:hidden;">
    <center>
   

        <div id="mch1">
            
            <div class="widTitle">
                <span class="large">@ViewBag.headline</span>
                <span class="small">&raquo;</span>
            </div>
            


            @{ int count = 0; }
            @foreach (var item in Model)
            {   
                <a class="widItem1" onclick="createFlashMarkup('@(item.enclosure)&volume=80');">
                    <img alt="" src="@item.image" />
                    <div>
                        <span><b>@item.title</b></span>
                        <span style=" color:#757575;">@item.pub_date</span>
                    </div>
                </a>
                
                if (count == 5)
                {
                    break;
                }
                
                count += 1;
            }

        </div>


    </center>




    <script type="text/javascript">
        function createFlashMarkup(uri) {
            window.parent.postMessage(uri, "http://sakson.info");
        }
    </script>

    




    <script type="text/javascript">
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-28695705-1', 'auto', { 'name': 'baron', 'legacyCookieDomain': 'hypster.com' });
        ga('baron.send', 'pageview');
        ga('create', 'UA-99868480-1', 'auto', { 'name': 'junemedia' });
        ga('junemedia.send', 'pageview');
    </script>
   
</body>
</html>

