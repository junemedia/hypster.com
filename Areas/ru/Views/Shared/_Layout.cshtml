<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <meta name="description" content="@ViewBag.Desc Создайте Tumblr плейлист, музыка для Tumblr." />
    <meta name="keywords" content="hypster, музыка, радио, телевидение, Tumblr, Blogger, бесплатный плейлист, плеер, плеер для Tumblr, жанров, музыку бесплатно онлайн, список воспроизведения, плеер скины, плейлист онлайн, Tumblr Плейлист, создавать плейлист Tumblr, музыка плейлист для Tumblr ,музыку плейлист" />




    <script src="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css"  type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-1.8.3.min.js"  type="text/javascript"></script>
    <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"  type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>


    <script src="/Areas/ru/scripts/hypScript.js?id=87216111200" type="text/javascript"></script>
    <script src="/Areas/ru/scripts/hypSlide.js?id=87216111200" type="text/javascript"></script>

    
    <link href="/Areas/ru/css/Site.css?id=87216111200" rel="stylesheet" type="text/css" />
    


    

    <script type="text/javascript">
        if (isMobile.any()) {
            if ($(window).width() < 590) {
                window.location = "http://mobile.@System.Configuration.ConfigurationManager.AppSettings["hypsterHostName"]";
            }
        }

        $(document).ready(function () {
            RenderLayout();

            $("#langBar").css("display", "block");
            $("#langBar").css("left", ($(window).width()-90) + "px");
        });

        $(window).resize(function () {
            RenderLayout();

            $("#langBar").css("display", "block");
            $("#langBar").css("left", ($(window).width()-90) + "px");
        });
    </script>

</head>





<body>
    <div class="MainBD13">
        

        <div class="ContentHeaderCont">
           
                <div class="ContentHeaderInner">
                    
                    
                    <div id="MainMenu" style=" z-index:99999;" >
                        <img class="mLogo" alt="Hypster Logo" src="/imgs/shared/main_hyp_logo.png" onclick="window.location='/ru/Главная';" />
                        

                        <div class="hypContHolder" onmouseover="$('#HypsterSub').css('display','block');" onmouseout="$('#HypsterSub').css('display','none');">
                            <a class="mHypster" href="/ru/Главная" >Главная</a>
                            
                            <div id="HypsterSub" style=" display:none;">
                                <a class="HypSub_tv" href="http://@System.Configuration.ConfigurationManager.AppSettings["videoHostName"]">ТВ</a>
                                <a class="HypSub_news" href="/content/HypNews">Новости</a>
                            </div>
                        </div>


                        <div class="listenContHolder" onmouseover="$('#ListenSub').css('display','block');" onmouseout="$('#ListenSub').css('display','none');">
                            <a class="mListen" href="/ru/Слушать">Слушать</a>

                            <div id="ListenSub" style=" display:none;">
                                <a class="HypSub_listen" href="/ru/Слушать">Искать Музыку</a>
                                <a class="HypSub_charts" href="/ru/Чарты">Чарты</a>
                                <a class="HypSub_festivals" href="/ru/Фестивали">Фестивали</a>
                            </div>
                        </div>


                        <div class="createContHolder" onmouseover="$('#CreateSub').css('display','block');" onmouseout="$('#CreateSub').css('display','none');">
                            <a class="mCreate" href="@if (User.Identity.IsAuthenticated){<text>/ru/Создать/Плейлист</text>}else{<text>/ru/Создать</text>}">Создать</a>

                            <div id="CreateSub" style=" display:none;">
                                <a class="HypSub_player" href="/ru/Создать/Плеер">Плеер</a>
                                <a class="HypSub_playlist" href="/ru/Создать/Плейлист">Плейлист</a>
                                <a class="HypSub_radio" href="/ru/Создать/Станцию">Станцию</a>
                            </div>
                        </div>

                        <a class="mConnect" href="/ru/Соединить">Соединить</a>


                        <div class="TopHeaderCont">
                            @Html.Partial("~/Areas/ru/Views/Аккаунт/pr_LoginStatus.cshtml")
                        </div>
                    </div>
                    
                </div>

                <div id="header_Bar" style=" float:left; width:2px; height:23px; margin-top:-3px; background-color:#2e2e2e; border-bottom:3px solid #191919;"></div>
            
        </div>



        <div class="ContentRootCont" >
             @RenderBody()
        </div>
        
    
        <div class="push"></div>
    </div>





    
    <div class="ContentFooterCont">
        <center>

                <div class="BottomPane">
                    <div style=" float:left; width:290px; height:250px; margin-top:10px;">
                        <img alt="" src="/imgs/shared/footer_logo_1.png" style=" padding:13px 5px 15px 50px;" />


                        <div style=" float:left; width:100%; height:22px;">
                                <div id="hyp_sosialHolder1" onmouseover="this.style.display = 'none'; $('#hyp_SNR_BtnHolder_1').css('display','block');">
                                    <div class="hyp_SI_Cont_google"></div>
                                    <div class="hyp_SI_Cont_twitter"></div>
                                    <div class="hyp_SI_Cont_facebook"></div>
                                </div>

                                <div id="hyp_SNR_BtnHolder_1" style=" float:left; width:100%; display:none;">
                    
                                    <div class="hyp_SI_Cont_facebook1">
                                        <iframe src="//www.facebook.com/plugins/like.php?href=http%3A%2F%2F@(System.Configuration.ConfigurationManager.AppSettings["newsHostName"])&amp;send=false&amp;layout=button_count&amp;width=90&amp;show_faces=false&amp;action=like&amp;colorscheme=light&amp;font&amp;height=21" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:90px; height:21px;" allowTransparency="true"></iframe>
                                    </div>

                                    <div class="hyp_SI_Cont_twitter1">
                                        <a href="https://twitter.com/share" class="twitter-share-button" data-url="http://@System.Configuration.ConfigurationManager.AppSettings["newsHostName"]" data-text="Hypster">Tweet</a>
                                        <script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = "//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs); } } (document, "script", "twitter-wjs");</script>
                                    </div>

                                    <div class="hyp_SI_Cont_google1">
                                        <div class="g-plusone" data-size="medium" data-href="http://@System.Configuration.ConfigurationManager.AppSettings["newsHostName"]"></div>
                                        <script type="text/javascript">                                            (function () { var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true; po.src = 'https://apis.google.com/js/plusone.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s); })();</script>
                                    </div>

                                </div>
                        </div>

                    </div>


                    <div class="BottomPaneInnr">

                        <div class="MenuItmCont">
                            <a href="/ru/Хипстер/Плеер_для_Тумблера">Плеер для Тумблера</a>
                            <a href="/ru/Хипстер/Как_использовать" >Как использовать сайт</a>
                            <a href="/ru/Хипстер/Хипстер_на_мобильном" >Хипстер на мобильном</a>
                        </div>


                        <div class="MenuItmCont">
                            <a href="/ru/Главная" >Хипстер</a>
                            <a href="/ru/Слушать" >Слушать</a>
                            <a href="/ru/Создать" >Создать</a>
                            <a href="/ru/Соединить" >Соединить</a>
                        </div>

                        <div class="MenuItmCont">
                            <a href="/ru/Слушать/Радио" >Радио</a>
                            <a href="http://@System.Configuration.ConfigurationManager.AppSettings["videoHostName"]" >ТВ</a>
                            <a href="http://@System.Configuration.ConfigurationManager.AppSettings["newsHostName"]" >Новости</a>
                        </div>
                        
                        <div class="MenuItmCont">
                            <a href="/ru/Хипстер/Контакты" >Контакты</a>
                            <a href="/ru/Хипстер/Про_Нас" >Про Нас</a>
                            <a href="/ru/Хипстер/Конфиденциальность" >Конфиденциальность</a>
                            <a href="/ru/Хипстер/Пользовательское_Соглашение" >Соглашение</a>
                        </div>


                    
                        <div style=" float:right; width:100%;">
                            <div class="FeaturedAdContBottom">
                                <div id="TopAdd_Bottom_2938eu93">
                                    @Html.Partial("~/Views/hypAds/ad_728x90_btf.cshtml")
                                </div>
                                <!-- 06_25 -->
                                
                            </div>
                        </div>

                    </div>



                    <div style=" float:left; width:98%; height:15px; margin-top:-10px; font-size:9px; text-align:right; ">@System.Configuration.ConfigurationManager.AppSettings["server_ID"]</div>
                </div>

        </center>
    </div>
    



    <div id="langBar">
        <a class="langItm" onclick="if($('#OLangs').css('display')=='none'){ $('#OLangs').css('display','block'); }else{ $('#OLangs').css('display','none'); }">RU</a>

        <div id="OLangs" style=" display:none; float:right;">
            <a class="langItm" href="/">EN</a>
            <a class="langItm" href="/es/Casa">ES</a>
        </div>
    </div>





    
    <div class="Trans_BACK" id="HideBackDiv" onclick="Show_Content();"></div>
    <div id="popupContainer"></div>
    


    




    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-28695705-1']);
        _gaq.push(['_setDomainName', 'hypster.com']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

</script>
    
   
</body>
</html>
